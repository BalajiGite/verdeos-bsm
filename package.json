{
  "name": "notus-nextjs",
  "homepage": "https://demos.creative-tim.com/notus-nextjs",
  "version": "1.1.0",
  "description": "VerdeOS BPM.",
  "repository": "https://github.com/creativetimofficial/notus-nextjs",
  "license": "MIT",
  "dependencies": {
    "@aws-amplify/ui-react": "^5.3.1",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@popperjs/core": "2.9.1",
    "aws-amplify": "^5.3.11",
    "axios": "^1.5.1",
    "chart.js": "^3.9.1",
    "concurrently": "^8.2.2",
    "gulp": "^4.0.2",
    "gulp-append-prepend": "1.0.8",
    "js-cookie": "^3.0.5",
    "next": "^12.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "next start",
    "build": "next build",
    "dev": "concurrently \"next dev\" \"npm run watch:tailwind\"",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm run dev",
    "build:tailwind": "tailwind build styles/index.css -o styles/tailwind.css",
    "watch:tailwind": "tailwindcss --watch --input styles/index.css --output styles/tailwind.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "typescript": "4.2.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3"
  }
}
